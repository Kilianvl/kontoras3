    "@types/session-file-store": "^1.2.5",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nexe": "github:WuerzburgerMaetschke/nexe",
    "nodemon": "^3.1.3",
    "tsx": "^4.11.0",
    "typescript": "~5.7.3"
  }
